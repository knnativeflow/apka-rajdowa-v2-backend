{
  "name": "native-flow-express-ts-boilertemplate",
  "version": "1.0.0",
  "description": "Boilertemplate of express + typescript REST backend app",
  "author": "Maciej KopeÄ‡ (dimitor115)",
  "license": "MIT",
  "main": "dist/server.js",
  "scripts": {
    "dev": "nodemon --ignore src/api/_auto/routes.ts --watch src -e ts --exec npm run dev-start",
    "dev-start": "npm run tsoaGen && ts-node -r tsconfig-paths/register src/server.ts",
    "tsoaGen": "ts-node -r tsconfig-paths/register apiGenerator.ts",
    "build": "npm run tsoaGen && ttsc && ts-node copyStaticAssets.ts",
    "lint": "eslint --ext src/**/*.ts",
    "lint:fix": "eslint --fix src/**/*.ts",
    "start": "node dist/server.js",
    "test": "jest",
    "playground": "ts-node tsconfig-paths/register src/playground.ts"
  },
  "engines": {
    "npm": "6.x.x",
    "node": "12.x.x"
  },
  "devDependencies": {
    "@types/cors": "^2.8.6",
    "@types/dotenv": "^6.1.1",
    "@types/express": "^4.17.1",
    "@types/jest": "^24.0.18",
    "@types/jsonwebtoken": "^8.3.4",
    "@types/mongoose": "^5.5.12",
    "@types/mongoose-paginate-v2": "^1.0.5",
    "@types/node": "^12.0.12",
    "@types/shelljs": "^0.8.0",
    "@types/swagger-ui-express": "^3.0.1",
    "@types/uuid": "^3.4.5",
    "@typescript-eslint/eslint-plugin": "^2.15.0",
    "@typescript-eslint/parser": "^2.15.0",
    "@zerollup/ts-transform-paths": "^1.7.11",
    "eslint": "^5.16.0",
    "nodemon": "^1.19.1",
    "shelljs": "^0.8.2",
    "ts-node": "^8.3.0",
    "tsconfig-paths": "^3.9.0",
    "tslint": "^5.18.0",
    "ttypescript": "^1.5.10",
    "typescript": "^3.7.1"
  },
  "dependencies": {
    "@types/multer": "^1.4.2",
    "@types/qs": "^6.9.0",
    "body-parser": "^1.19.0",
    "cors": "^2.8.5",
    "dotenv": "^8.0.0",
    "exceljs": "^3.8.1",
    "express": "^4.17.1",
    "google-auth-library": "^5.9.2",
    "jsonwebtoken": "^8.5.1",
    "mongoose": "^5.6.8",
    "mongoose-paginate-v2": "^1.3.3",
    "mongoose-slug-generator": "^1.0.4",
    "multer": "^1.4.2",
    "qs": "^6.9.1",
    "reflect-metadata": "^0.1.13",
    "swagger-ui-express": "^4.0.7",
    "tsoa": "^2.5.13",
    "winston": "^3.2.1"
  }
}
